<h1>Edit Info:</h1>
<form action="/Artists/Details/@(Model.ArtistId)" method="post">
  <label for="name">Artist's Name</label>
  <input id="name" name="name" type="text" value="@(Model.Name)">
  <label for="age">Artist's Age</label>
  <input id="age" name="age" type="number" value="@(Model.Age)">
  <label for="email">Artist's Email</label>
  <input id="email" name="email" type="text" value="@(Model.Email)">
  <label for="bio">Artist's Bio</label>
  <input id="bio" name="bio" type="text" value="@(Model.Bio)">
  <label for="seeking">Seeking</label>
  <input id="seeking" name="seeking" type="text" value="@(Model.Seeking)">
  <label for="past-work">Past Work</label>
  <input id="past-work" name="past-work" type="text" value="@(Model.PastWork)">
  <button type="submit">Go!</button>
</form>

<a href="/Artists/Details/@(Model.ArtistId)"><p>Return to Details</p></a>